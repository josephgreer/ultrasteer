keywords = {GPU/CUDA,interactive volume visualization,multiresolution rendering,multiscale,tensor reconstruction},
pages = {2135--2143},
pmid = {22034332},
title = {{Interactive multiscale tensor reconstruction for multiresolution volume visualization}},
volume = {17},
year = {2011}
}
