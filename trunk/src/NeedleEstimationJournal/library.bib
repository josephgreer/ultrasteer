@article{Cosgrove2010,
abstract = {Ultrasound can be used to image perfusion in two ways: the traditional one using Doppler and the more recent using microbubble contrast agents. Doppler is simple to use and inexpensive but is limited to larger vessels with faster flow rates. It cannot interrogate the microvasculature because bulk tissue movement is faster than capillary flow. It has been used for liver and tumour flow. Contrast studies are much richer and can assess both the macro- and microcirculation. One approach analyses the time-intensity curves in a region of interest, e.g. a tumour, myocardium, brain, following bolus i.v. injection. Another approach measures the time taken for the microbubbles to cross a vascular bed of interest. These arrival times can be useful for the liver in both diffuse and focal diseases and for the kidney. Features derived from time-intensity curves following bolus i.v. injections of microbubbles form sensitive early indicators of the vascular response of tumours to antivascular drugs. This approach, known as dynamic contrast-enhanced ultrasound (DCE-US), has been accepted as a valid technique for monitoring tumour response by several authorities.},
author = {Cosgrove, David and Lassau, Nathalie},
doi = {10.1007/s00259-010-1537-7},
file = {:F$\backslash$:/Documents and Settings/Joey/Desktop/papers/stanford/ultrasound perfusion/art\%3A10.1007\%2Fs00259-010-1537-7.pdf:pdf},
issn = {1619-7089},
journal = {European journal of nuclear medicine and molecular imaging},
keywords = {Animals,Humans,Inflammation,Inflammation: physiopathology,Inflammation: ultrasonography,Microbubbles,Neoplasms,Neoplasms: blood supply,Neoplasms: ultrasonography,Perfusion Imaging,Perfusion Imaging: methods,Ultrasonics,Ultrasonography, Doppler},
month = aug,
number = {July},
pages = {S65--85},
pmid = {20640418},
title = {{Imaging of perfusion using ultrasound.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20640418},
volume = {37 Suppl 1},
year = {2010}
}
